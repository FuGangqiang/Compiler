TOKEN(TOK_GT, ">")
TOKEN(TOK_GE, ">=")
TOKEN(TOK_EE, "==")
TOKEN(TOK_NE, "!=")
TOKEN(TOK_AND_AND, "&&")
TOKEN(TOK_OR_OR, "||")
