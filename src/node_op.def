#ifndef OP
#error "You must define OP macro before include this file"
#endif

/* unary */
OP(OP_UNA_DEREF, "*")
/* logical inversion, bitwise not */
OP(OP_UNA_NOT, "!")
OP(OP_UNA_NEG, "-")

/* binary */
OP(OP_BIN_ADD, "+")
OP(OP_BIN_SUB, "-")
OP(OP_BIN_MUL, "*")
OP(OP_BIN_DIV, "/")
OP(OP_BIN_REM, "%")
OP(OP_BIN_AND, "&&")
OP(OP_BIN_OR, "||")
OP(OP_BIN_BIT_AND, "&")
OP(OP_BIN_BIT_OR, "|")
OP(OP_BIN_BIT_XOR, "^")
OP(OP_BIN_SHL, "<<")
OP(OP_BIN_SHR, ">>")
OP(OP_BIN_EQ, "==")
OP(OP_BIN_NE, "!=")
OP(OP_BIN_LT, "<")
OP(OP_BIN_LE, "<=")
OP(OP_BIN_GT, ">")
OP(OP_BIN_GE, ">=")
/* `m..n` */
OP(OP_BIN_RANGE, "..")
/* `m..=n` */
OP(OP_BIN_RANGE_EQ, "..=")
